{
  "name": "discord-ts-boilerplate",
  "version": "0.1.0",
  "description": "Discord bot built in TypeScript using discord.js",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "lint": "eslint src/**/*.ts",
    "clear-commands": "bun scripts/register_commands.ts --clear-commands",
    "build": "bun build --outfile=dist/index.js --target=bun src/index.ts",
    "deploy": "bun scripts/register_commands.ts",
    "start": "bun dist/index.js",
    "prisma": "prisma"
  },
  "keywords": [
    "discordbot",
    "discord.js",
    "typescript"
  ],
  "author": "Maximus7474",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "6.15.0",
    "colors": "^1.4.0",
    "discord.js": "^14.20.0",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/bun": "^1.3.6",
    "@types/node": "^22.15.32",
    "eslint": "^9.29.0",
    "prisma": "^6.15.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  }
}
